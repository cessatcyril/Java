<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.cyril.afpa.gui.JeuDeLigneController">
   <children>
      <VBox spacing="30.0" AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="30.0">
         <children>
            <HBox spacing="20.0">
               <children>
                  <Label prefHeight="25.0" text="Choix du fournisseur" />
                  <ComboBox fx:id="fournisseur" onAction="#choixFournisseur" prefHeight="25.0" prefWidth="250.0" promptText="Choisissez" />
               </children>
            </HBox>
            <TextArea fx:id="textResultat" prefHeight="200.0" prefWidth="200.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
